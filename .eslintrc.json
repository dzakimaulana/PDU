{
  "env": {
    "node": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      1
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  },
  "parserOptions": {
    "ecmaVersion": 12
  }
}